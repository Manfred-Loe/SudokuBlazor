	"version": "0.2.0",
	"configurations": [
		{
            "name": "Console Tester",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ConsoleTester/bin/Debug/net6.0/ConsoleTester.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ConsoleTester",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
		{
			"name": "Launch and Debug Standalone Blazor WebAssembly App",
			"type": "blazorwasm",
			"request": "launch",
			"cwd": "${workspaceFolder}/MudSudoku"
		}
	]
}